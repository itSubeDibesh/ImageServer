{
    "logger": {
        "enableLogger": true,
        "maxLogSize": 5,
        "logInConsole": false
    },
    "server": {
        "PORT": 8080,
        "isHttp": true,
        "session": {
            "name": "IMAGE_SERVER.SID",
            "secret": "sessionSecret@Image _Sever.2022",
            "resave": true,
            "saveUninitialized": false,
            "cookieMaxAge": 864e5,
            "secureCookie": true
        },
        "preventions": {
            "sqLInjection": true,
            "enableAccessControl": true,
            "enableCsrf": false,
            "limitRequest": {
                "state": false,
                "limit": {
                    "request": 2e2,
                    "periodInMs": 3e4
                }
            }
        }
    },
    "image": {
        "extensions": [
            "jpg",
            "jpeg",
            "png",
            "gif",
            "bmp",
            "svg",
            "webp",
            "ico",
            "ttif",
            "pdf"
        ],
        "maxUploads": 10,
        "maxFileSize": 10485760,
        "storagePath": "storage/"
    },
    "database": {
        "scriptName": "ImageServer"
    }
}