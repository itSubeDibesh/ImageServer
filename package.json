{
  "name": "image_server",
  "version": "1.0.0",
  "description": "A image server for cybersecurity assignment CET324",
  "main": "index.js",
  "directories": {
    "backend": "backend",
    "configuration": "configuration",
    "database": "database",
    "documents": "documents",
    "frontend": "frontend",
    "library": "library"
  },
  "scripts": {
    "setup:frontend": "CD ./frontend && npm install",
    "setup:backend": "npm install",
    "setup": "npm run setup:backend && npm run setup:frontend",
    "clean": "set WORK_ENV=development && node cleanup.js",
    "dev:frontend": "set WORK_ENV=development && cd frontend && npm run serve",
    "dev:backend": "set WORK_ENV=development && nodemon --ignore logs/ --ignore tests/ --ignore backend/config/ && index.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:frontend": "set WORK_ENV=production && cd frontend && npm run build",
    "serve:backend": "set WORK_ENV=production && node index.js",
    "serve": "set WORK_ENV=production && npm run build:frontend && npm run serve:backend"
  },
  "keywords": [
    "Registration",
    "Server",
    "Security"
  ],
  "license": "MIT",
  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "express-validator": "^6.14.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4-lts.1",
    "node-fetch": "^3.2.5",
    "nodemailer": "^6.7.5",
    "snipercode.filesystem": "^1.0.1",
    "sqlite3": "^5.0.8",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^7.2.0",
    "nodemon": "^2.0.16",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itSubeDibesh/ImageServer.git"
  },
  "author": "Dibesh Raj Subedi",
  "bugs": {
    "url": "https://github.com/itSubeDibesh/ImageServer/issues"
  },
  "homepage": "https://github.com/itSubeDibesh/ImageServer#readme"
}
